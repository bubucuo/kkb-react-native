+    BOOL _isBoundsInit;
+}
+
+- (instancetype)init {
+    _isBoundsInit = NO;
+    _markers = [NSMutableDictionary new];
+    self = [super init];
+    return self;
+}
+
+- (void)setFrame:(CGRect)frame {
+    if (!_isBoundsInit) {
+        [super setFrame:frame];
+    }
+}
+
